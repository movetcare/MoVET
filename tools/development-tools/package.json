{
  "name": "development-tools",
  "version": "0.0.0",
  "description": "MoVET Platform Tools",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/*.ts",
    "update": "ncu -u --reject 'axios,public-ip'",
    "build": "pnpm build:tools && pnpm run build:api:package:constant && pnpm run build:api:package:types",
    "build:tools": "cd ../../ && cp -a firebase.json ./tools/development-tools/src/utils/ && cd tools/development-tools && tsc",
    "build:api:package:constant": "cp -a ../../packages/constant/src/. ../../apps/api.movetcare.com/src/constant",
    "build:api:package:types": "cp -a  ../../packages/types/src/. ../../apps/api.movetcare.com/src/types",
    "watch": "tsc --watch --preserveWatchOutput",
    "init:config:dev": "pnpm config:all && pnpm init:test_user && pnpm config:users && pnpm config:breeds && pnpm config:terminals && pnpm config:reasons && pnpm config:reason_groups && pnpm config:cancellation_reasons && pnpm config:shifts && sleep 3 && curl -i -v GET http://localhost:5001/movet-care-staging/us-central1/taskRunnerDev",
    "init:test_user": "sleep 3 && curl -i -v POST http://localhost:5001/movet-care-staging/us-central1/initTestUser",
    "config:all": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:users": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"users\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:terminals": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"terminals\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:reasons": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"reasons\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:reason_groups": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"reason_groups\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:cancellation_reasons": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"cancellation_reasons\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:breeds": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"breeds\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:shifts": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"shifts\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:invoices": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"invoices\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:appointments": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"appointments\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:items": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"items\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "init:config:stage": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST https://us-central1-movet-care-staging.cloudfunctions.net/incomingWebhook/app/config/",
    "init:config:prod": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST https://us-central1-movet-care.cloudfunctions.net/incomingWebhook/app/config/",
    "init:config:local:prod": "sleep 3 && curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST http://localhost:5001/movet-care/us-central1/incomingWebhook/app/config/",
    "dev": "pnpm run build && concurrently -n 'DEV:WEBHOOKS,DEV:CRON,BUILD:API,API,PACKAGE:CONSTANTS:BUILD,PACKAGE:UTILITIES:BUILD,PACKAGE:TYPES:WATCH,PACKAGE:CONSTANT:WATCH,TESTING:CYPRESS' 'sleep 30 && pnpm run init:config:dev && op run --no-masking --env-file='.env' -- pnpm run webhooks' 'sleep 30 && op run --no-masking --env-file='.env' -- pnpm run cron' 'cd ../../apps/api.movetcare.com && pnpm run watch' 'pnpm run watch' 'cd ../../packages/constant && pnpm run watch' 'cd ../../packages/utilities && pnpm run watch' 'pnpm run watch:package:type' 'pnpm run watch:package:constant' 'cd ../../ && pnpm cypress open'",
    "dev:test": "pnpm run build && concurrently -n 'DEV:WEBHOOKS' 'sleep 30 && pnpm run init:config:dev && op run --no-masking --env-file='.env' -- pnpm run webhooks'",
    "cron": "cd build && node ./runCron.js",
    "webhooks": "cd build && node ./ngrok.js",
    "watch:package:type": "watch 'pnpm run build:api:package:types' ../../packages/types",
    "watch:package:constant": "watch 'pnpm run build:api:package:constant' ../../packages/constant"
  },
  "author": "MoVET",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "0.27.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.3.1",
    "firebase": "^9.23.0",
    "firebase-admin": "^11.9.0",
    "firebase-functions": "^4.4.1",
    "fs-extra": "^11.1.1",
    "kill-port": "^2.0.1",
    "ngrok": "^4.3.3",
    "public-ip": "4.0.4",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "typescript": "^5.1.3"
  }
}

{
  "name": "movet",
  "version": "3.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start",
    "start:prod": "npx expo start --no-dev --minify",
    "tunnel": "npx expo start --tunnel",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "build:ios": "eas build --profile release --platform ios",
    "build:android": "eas build --profile release --platform android",
    "clean": "rm -rf node_modules && watchman watch-del-all && rm -fr $TMPDIR/haste-map-* && rm -rf $TMPDIR/metro-cache && npm i && npx expo-doctor && npx expo install --check",
    "check:deps": "npx expo install --check",
    "update": "ncu -u && npm i && npm run clean",
    "update:version": "node ./incrementVersion.js",
    "build:all": "npm run build:ios && npm run build:android",
    "deploy:ios": "eas submit --platform ios --latest --profile release",
    "deploy:android": "eas submit --platform android --latest --profile release",
    "deploy:all": "eas submit --platform all --latest --profile release",
    "convert-svgs-to-jpgs": "npx @svgr/cli --out-dir ./assets/images/icons --ignore-existing ./assets/images/svgs"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@logrocket/react-native": "^1.27.3",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-picker/picker": "2.6.1",
    "@react-navigation/native": "^6.1.9",
    "@stripe/stripe-react-native": "0.34.0",
    "expo": "^50.0.0-preview.10",
    "expo-constants": "~15.4.3",
    "expo-device": "~5.9.2",
    "expo-font": "~11.10.0",
    "expo-image-picker": "~14.7.1",
    "expo-linking": "~6.2.2",
    "expo-network": "~5.8.0",
    "expo-notifications": "~0.27.3",
    "expo-router": "~3.4.2",
    "expo-splash-screen": "~0.26.2",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.1",
    "firebase": "^10.7.1",
    "lodash.debounce": "^4.0.8",
    "lottie-react-native": "5.1.6",
    "pullstate": "^1.25.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-native": "0.73.2",
    "react-native-autocomplete-dropdown": "^3.1.3",
    "react-native-get-random-values": "~1.8.0",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "1.8.0",
    "react-native-mask-text": "^0.14.2",
    "react-native-reanimated": "~3.6.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-webview": "13.6.4",
    "tailwindcss": "^3.4.1",
    "twrnc": "^3.6.7",
    "uuid": "^9.0.1",
    "yup": "^1.3.3",
    "expo-build-properties": "~0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@expo/metro-config": "~0.16.0",
    "@svgr/cli": "^8.1.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "~18.2.47",
    "@types/uuid": "^9.0.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "husky": "^8.0.3",
    "react-native-svg-transformer": "^1.3.0",
    "semver": "^7.5.4",
    "typescript": "^5.3.3"
  },
  "private": true
}

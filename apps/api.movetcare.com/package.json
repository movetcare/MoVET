{
  "name": "api.movetcare.com",
  "version": "0.0.0",
  "private": true,
  "main": "lib/index.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "update": "ncu -u --reject axios",
    "lint": "eslint src/",
    "fix": "eslint src/ --fix",
    "build": "pnpm run config:env && tsc",
    "config:env": "op inject -i runtimeconfig.json -o .runtimeconfig.json --force",
    "dev": "pnpm run config:env && pnpm run emulate",
    "watch": "tsc --watch --preserveWatchOutput",
    "emulate": "firebase emulators:start --only auth,functions,firestore,pubsub,storage",
    "start": "pnpm run dev",
    "init:config:dev": "curl -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/ && curl -v POST http://localhost:5001/movet-care-staging/us-central1/initTestUser",
    "init:config:stage": "curl -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST https://us-central1-movet-care-staging.cloudfunctions.net/incomingWebhook/app/config/",
    "init:config:prod": "curl -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST https://us-central1-movet-care.cloudfunctions.net/incomingWebhook/app/config/",
    "init:config:local:prod": "curl -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST http://localhost:5001/movet-care/us-central1/incomingWebhook/app/config/",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.7.0",
    "@sendgrid/client": "^7.7.0",
    "@sendgrid/eventwebhook": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.19.0",
    "@slack/web-api": "^6.8.0",
    "axios": "0.27.2",
    "cors": "^2.8.5",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "firebase-admin": "^11.2.1",
    "firebase-functions": "^4.0.2",
    "format-json": "^1.0.3",
    "json2csv": "^5.0.7",
    "semver": "^7.3.8",
    "stripe": "^10.17.0",
    "trello-node-api": "^0.0.9",
    "twilio": "^3.83.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^18.11.9",
    "@types/safe-compare": "^1.1.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  }
}

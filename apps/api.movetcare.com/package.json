{
  "name": "api.movetcare.com",
  "version": "0.0.0",
  "private": true,
  "main": "lib/index.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "update": "ncu -u --reject 'axios,eslint,@headlessui/react'",
    "lint": "eslint src/",
    "fix": "eslint src/ --fix",
    "build": "tsc",
    "config:env": "op inject -i runtimeconfig.json -o .runtimeconfig.json --force",
    "dev": "pnpm run config:env && pnpm run emulate",
    "dev:test": "pnpm run config:env && pnpm run emulate",
    "watch": "tsc --watch --preserveWatchOutput",
    "emulate": "firebase emulators:start --only auth,functions,firestore,pubsub,storage",
    "start": "pnpm run dev",
    "init:config:dev": "pnpm config:all && pnpm init:test_user && pnpm config:users && pnpm config:terminals && pnpm config:reasons && pnpm config:reason_groups && pnpm config:breeds && pnpm config:invoices && pnpm config:appointments && pnpm config:items",
    "init:test_user": "curl -i -v POST http://localhost:5001/movet-care-staging/us-central1/initTestUser",
    "config:all": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:users": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"users\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:terminals": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"terminals\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:reasons": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"reasons\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:reason_groups": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"reason_groups\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:breeds": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"breeds\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:invoices": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"invoices\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:appointments": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"appointments\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:items": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"items\"}' POST http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/app/config/",
    "config:goto:auth": "curl -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\"}' -X GET http://localhost:5001/movet-care-staging/us-central1/incomingWebhook/goto/login/",
    "init:config:stage": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST https://us-central1-movet-care-staging.cloudfunctions.net/incomingWebhook/app/config/",
    "init:config:prod": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST https://us-central1-movet-care.cloudfunctions.net/incomingWebhook/app/config/",
    "init:config:local:prod": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"type\":\"all\"}' POST http://localhost:5001/movet-care/us-central1/incomingWebhook/app/config/",
    "wipe:test:dev": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"id\":\"wipe_test_data\"}' POST http://localhost:5001/movet-care-staging/us-central1/resetTestData/",
    "wipe:test:prod": "curl -i -v -H 'Content-Type: application/json' -d '{\"apiKey\":\"L9At3HGmvRDuyi7TTX\",\"id\":\"wipe_test_data\"}' POST https://us-central1-movet-care.cloudfunctions.net/resetTestData/",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@sendgrid/client": "^8.1.3",
    "@sendgrid/mail": "^8.1.3",
    "@slack/web-api": "^7.0.4",
    "axios": "0.27.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "expo-server-sdk": "^3.10.0",
    "express": "^4.19.2",
    "firebase-admin": "^12.1.1",
    "firebase-functions": "^5.0.1",
    "simple-oauth2": "^5.0.0",
    "stripe": "^15.8.0",
    "trello-node-api": "^0.0.9",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.13.0",
    "@types/safe-compare": "^1.1.2",
    "@types/simple-oauth2": "^5.0.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  }
}
